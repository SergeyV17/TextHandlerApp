<Application x:Class="TextHandlerApp.App"
             xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
             xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
             xmlns:local="clr-namespace:TextHandlerApp"
             StartupUri="Views\MainWindow.xaml">
    <Application.Resources>
        <ResourceDictionary>
            <ResourceDictionary.MergedDictionaries>
                <ResourceDictionary Source="Views/ResourceDictionaries/GridDictionary.xaml"/>
                <ResourceDictionary Source="Views/ResourceDictionaries/TextBlockDictionary.xaml"/>
                <ResourceDictionary Source="Views/ResourceDictionaries/BorderDictionary.xaml"/>
                <ResourceDictionary Source="Views/ResourceDictionaries/TextBoxDictionary.xaml"/>
                <ResourceDictionary Source="Views/ResourceDictionaries/ImageDictionary.xaml"/>
                <ResourceDictionary Source="Views/ResourceDictionaries/StackPanelDictionary.xaml"/>
                <ResourceDictionary Source="Views/ResourceDictionaries/ProgressBarDictionary.xaml"/>
            </ResourceDictionary.MergedDictionaries>
        </ResourceDictionary>
    </Application.Resources>
</Application>
